#include<stdio.h>
#include<conio.h>
int main()
{
    int num,a,b=0,c,d,e;
    printf("Enter a number to check it is armstrong number or not.\n");
    scanf("%d",&num);
    c=num;
    while(num!=0)
    {
        a=num%10;
        b=b+(a*a*a);
        num=num/10;
    }
    if(b==c)
        printf("%d is a armstrong number.\n",c);
    else
        printf("%d is not a armstrong number.\n",c);
    getch();
}
